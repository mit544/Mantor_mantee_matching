{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./app/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/lib/matchAlgorithm.js", "app/backend/models/mentor.js", "app/backend/models/mentee.js", "app/lib/mongodb.ts", "app/api/mentor/route.js", "app/api/match/route.ts", "app/api/mantee/route.js", "app/src/components/forget_password_popup.tsx"],
  "exclude": ["node_modules"]
}
